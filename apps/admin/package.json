{
	"name": "cereza_admin",
	"version": "0.0.0",
	"main": "dist/index.js",
	"bin": {
		"cereza_admin": "./bin/run"
	},
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"dependencies": {
		"@oclif/core": "^1.13.10",
		"@oclif/plugin-help": "^5",
		"chalk": "^4.1.2",
		"client": "*",
		"inquirer": "8.0.1"
	},
	"devDependencies": {
		"oclif": "^3.2.22",
		"shx": "^0.3.3",
		"typescript": "^4.8.3",
		"zx": "^7.1.1"
	},
	"oclif": {
		"bin": "cereza_admin",
		"dirname": "cereza_admin",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topicSeparator": " ",
		"update": {
			"s3": {
				"bucket": "cereza-admin"
			}
		}
	},
	"scripts": {
		"version": "oclif readme && git add README.md",
		"lint": "eslint . --ext .ts --config .eslintrc",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"posttest": "yarn run lint",
		"build": "shx rm -rf dist && tsc -b",
		"pretarball": "zx ../../scripts/import.mjs",
		"prepack": "yarn run build && oclif manifest && oclif readme",
		"postpack": "shx rm -f oclif.manifest.json"
	},
	"engines": {
		"node": ">=12.0.0"
	}
}
