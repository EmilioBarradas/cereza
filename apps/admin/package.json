{
	"name": "admin",
	"version": "0.0.0",
	"description": " ",
	"author": "Emilio Barradas @EmilioBarradas",
	"bin": {
		"admin": "./bin/run"
	},
	"homepage": "https://github.com/EmilioBarradas/cereza-admin",
	"license": "MIT",
	"main": "dist/index.js",
	"repository": "EmilioBarradas/cereza-admin",
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"dependencies": {
		"@oclif/core": "^1.13.10",
		"@oclif/plugin-help": "^5",
		"chalk": "^4.1.2",
		"client": "*"
	},
	"devDependencies": {
		"@oclif/test": "^2",
		"@types/chai": "^4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.4",
		"chai": "^4",
		"eslint": "^7.32.0",
		"eslint-config-oclif": "^4",
		"eslint-config-oclif-typescript": "^1.0.2",
		"globby": "^11",
		"mocha": "^9",
		"oclif": "^3.2.21",
		"shx": "^0.3.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.8.3"
	},
	"oclif": {
		"bin": "admin",
		"dirname": "admin",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		},
		"update": {
			"s3": {
				"bucket": "cereza"
			}
		}
	},
	"scripts": {
		"version": "oclif readme && git add README.md",
		"lint": "eslint . --ext .ts --config .eslintrc",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"posttest": "pnpm run lint",
		"import:api": "shx mkdir ./dist/api && shx cp -r ../api/dist ./dist/api/dist && shx cp -r ../api/node_modules ./dist/api/node_modules && shx cp ../api/package.json ./dist/api/package.json",
		"import:web": "shx cp -r ../web/.next/standalone ./dist/web",
		"import": "pnpm run import:api && pnpm run import:web",
		"build": "shx rm -rf dist && tsc -b && pnpm run import",
		"prepack": "pnpm run build && oclif manifest && oclif readme",
		"postpack": "shx rm -f oclif.manifest.json"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"bugs": "https://github.com/EmilioBarradas/cereza-admin/issues",
	"keywords": [
		"oclif"
	],
	"types": "dist/index.d.ts"
}
